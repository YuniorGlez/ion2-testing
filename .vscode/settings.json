// Coloque su configuración en este archivo para sobrescribir la configuración predeterminada y de usuario.
{
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/platforms/**": true,
        "**/www/**": true,
        "**/resources/**": true
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        ".editorconfig": true,
        "package-lock.json": true,
        "tsconfig.json": true,
        "tslint.json": true,
        "package.json": true,
        ".vscode": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/node_modules/**": true,
        "**/platforms/**": true,
        "**/www/**": true,
        "**/hooks/**": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/.git/": true,
        "**/www/": true
    }
}